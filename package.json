{
  "name": "kunyora",
  "version": "0.1.0",
  "description": "A node and browser based Rest request handler built to manage reqeusts in an effective and fast manner",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "scripts": {
    "clean": "rimraf lib dist es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/kunyora.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/kunyora.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "test": "jest",
    "watch:test": "jest --watchAll",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "rest",
    "reactJs",
    "angular",
    "vue",
    "request",
    "axios",
    "javascript"
  ],
  "author": [
    "Anifowoshe Gbenga David <daveanifowoshe@gmail.com> (https://github.com/gbenga504)",
    "Badewa Kayode <badewakayode1@gmail.com> (https://github.com/kayslay)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "lodash": "^4.17.4",
    "prettier": "1.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "camelcase": "^4.1.0",
    "invariant": "^2.2.2",
    "npm-badge": "^1.0.0"
  },
  "bugs": {
    "url": "https://github.com/kunyora/kunyora/issues"
  },
  "homepage": {
    "url": "https://kunyora.github.io/kunyora"
  },
  "unpkg": "dist/kunyora.js"
}
